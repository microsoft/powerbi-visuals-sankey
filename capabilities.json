        "cyclesLinks":{
            "displayName": "Cycles displaying",
            "displayNameKey": "Visual_Cycles",
            "properties": {
                "drawCycles": {
                    "displayName": "Duplicate nodes",
                    "displayNameKey": "Visual_DuplicateNodes",
                    "type": {
                        "enumeration": [
                            {
                                "displayName": "Duplicate",
