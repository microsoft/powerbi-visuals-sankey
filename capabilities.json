                    }
                },
                "fill": {
                    "displayName": "Color",
                    "displayNameKey": "Visual_LabelsFill",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "fontSize": {
                    "displayName": "Text Size",
                    "displayNameKey": "Visual_TextSize",
                    "type": {
                        "formatting": {
                            "fontSize": true
                        }
                    }
                }
            }
        },
        "links": {
            "displayName": "Links",
            "displayNameKey": "Visual_Links",
