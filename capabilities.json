                    "displayNameKey": "Visual_ScaleSettings",
                    "type": {
                        "bool": true
                    }
                },
                "provideMinHeight": {
                    "displayName": "Provide min optimal height of node",
                    "displayNameKey": "Visual_MinOptimalHeight",
